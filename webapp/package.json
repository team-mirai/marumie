  "dependencies": {
    "@next/third-parties": "^15.5.9",
    "@nivo/sankey": "^0.99.0",
    "@prisma/client": "^6.19.1",
    "@vercel/speed-insights": "^1.3.1",
    "apexcharts": "^5.3.6",
    "critters": "^0.0.25",
    "next": "15.5.9",
